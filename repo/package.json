{
  "name": "tcb-repo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "apps/*",
    "functions/*",
    "models/*",
    "webapps/*",
    "apis/*"
  ],
  "tcbRepo": {
    "type": "repo",
    "appRoot": "apps",
    "functionRoot": "functions",
    "apisRoot": "apis",
    "modelRoot": "models",
    "webappRoot": "webapps",
    "appMappings": {},
    "functionMappings": {},
    "apisMappings": {},
    "modelMappings": {},
    "webappMappings": {}
  }
}
